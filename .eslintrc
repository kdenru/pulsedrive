{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "settings": {
    "import/resolver": {
      "node": {
        "paths": ["src"]
      }
    }
  },
  "rules": {
    "semi": 0,
    "camelcase": 0,
    "arrow-parens": 0,
    "react/forbid-prop-types": 0,
    "react/state-in-constructor": 0,
    "react/jsx-one-expression-per-line": 0
  }
}